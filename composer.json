{
  "name": "ryoyoshihara/laravel-audit-diff",
  "description": "Eloquent model audit diff logger for Laravel.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/database": "^10.0|^11.0",
    "illuminate/filesystem": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "AuditDiff\\Laravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
     "AuditDiff\\Laravel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AuditDiff\\Laravel\\AuditServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
